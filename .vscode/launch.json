  "version": "0.2.0",
  "configurations": [
    {
      "name": "nest debug",
      "request": "launch",
      "runtimeExecutable": "npm",
      "args": ["run", "start:dev"],
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/nest"
    },
    {
      "name": "static server",
      "request": "launch",
      "runtimeExecutable": "npx",
      "args": ["http-server","-p 80"],
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/static-server"
    }
  ]
}
